.default-panel {    
background: #ff0000; color: #fff; 
}

.panel {
  width: 200px;
  background-color: #f9e497;  
  border: 1px solid #000;
  border-radius: 12px;
  box-shadow: 5px 5px 0 0 #a50000;
}
.menu {
  list-style: none;
  color: #a50000;
  font-family: 'Times', sans-serif;
  padding: 0 0 0 8px;
  margin: 10px 0 0 0;
}
.menu li {
  padding-bottom: 10px;
}
